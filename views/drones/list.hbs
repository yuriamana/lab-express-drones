<h2>Available drones</h2>
<ul>
  {{#each drones}}
    <li>
        <span>name: {{name}}<br>
        <span>propellers: {{propellers}}<br>
        <span>max speed: {{maxSpeed}}<br>
        {{!-- <a href="/drones/{{_id}}">create</a> --}}
        <a href="/drones/{{_id}}/edit/">edit</a>
        <form action="/drones/{{_id}}/delete/" method="post">
        <button>Delete</button>
        </form>
    </li>
  {{/each}}
</ul>